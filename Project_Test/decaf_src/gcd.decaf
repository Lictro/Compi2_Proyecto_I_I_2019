class GreatestCommonDivisor
{
	int a = 10;
	int b = 20;

	void main()
	{
		int x, y, z;
		x = a;
		y = b;
		z = gcd(x, y);

		System.out.println(z);
	}

	// Funcion que calcula el maximo comun divisor
	int gcd(int a, int b)
	{
		if (a == 0) {
			return b;
		} else {
			return gcd(b, a % b);
		}
	}
}